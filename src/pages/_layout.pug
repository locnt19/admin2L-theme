block var

doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1.0")
		meta(http-equiv="X-UA-Compatible" content="ie=edge")
		link(rel="stylesheet" href="./css/libraries-ui.min.css")
		link(rel="stylesheet" href="./css/all.min.css")
		title #{title}
		block link
	body.m4-cloak.h-vh-100
		.notify-container
		main(class="navview" data-role="navview" data-toggle="#paneToggle" data-expanded="xl" data-compact="lg" data-active-state="true")
			include ../partials/sidebar/sidebar.pug
			section.navview-content
				include ../partials/appbar/appbar.pug
				block main-content
		script(src="./js/libraries-ui.min.js")
		script(src="./js/all.min.js")
		block script
